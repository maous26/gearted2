{
  "expo": {
    "name": "gearted",
    "slug": "gearted",
    "scheme": "gearted",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/GEARTEDicon9.png",
      "resizeMode": "contain",
      "backgroundColor": "#2F4538"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.gearted",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Gearted a besoin de votre position pour trouver les points relais Mondial Relay à proximité.",
        "NSLocationAlwaysUsageDescription": "Gearted a besoin de votre position pour trouver les points relais Mondial Relay à proximité."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gearted.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Gearted a besoin de votre position pour trouver les points relais Mondial Relay à proximité."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "0b5e4bdd-08e9-4049-81ce-dbad5fd2efb0"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/0b5e4bdd-08e9-4049-81ce-dbad5fd2efb0"
    }
  }
}
